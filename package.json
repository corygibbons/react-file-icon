{
  "name": "react-file-icon",
  "version": "0.2.0",
  "description": "React component to render svg file icons",
  "author": "Pixel Union",
  "repository": {
    "url": "http://github.com/pixelunion/react-file-icon"
  },
  "main": "dist/main.js",
  "scripts": {
    "build": "rollup -c config/rollup.js",
    "prepublish": "npm run build",
    "start": "npm run styleguidist",
    "styleguidist": "styleguidist server --config config/styleguide/styleguidist.js",
    "styleguidist:build": "styleguidist build --config config/styleguide/styleguidist.js",
    "test": "jest --config config/jest.js"
  },
  "keywords": [
    "react",
    "component",
    "svg",
    "icons",
    "file types"
  ],
  "license": "MIT",
  "dependencies": {
    "lodash.uniqueid": "^4.0.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "tinycolor2": "^1.4.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.2",
    "babel-jest": "^22.4.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "component-image": "^1.1.0",
    "jest": "^22.4.3",
    "jest-image-snapshot": "^2.4.1",
    "react-styleguidist": "^6.5.3",
    "react-test-renderer": "^16.3.2",
    "rollup": "^0.54.1",
    "rollup-plugin-babel": "^3.0.4",
    "webpack": "^3.11.0",
    "webpack-blocks": "^1.0.0"
  }
}
