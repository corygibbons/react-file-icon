{
  "name": "react-file-icon",
  "version": "0.2.0",
  "description": "React component to render svg file icons",
  "repository": {
    "url": "http://github.com/corygibbons/react-file-icon"
  },
  "source": "src/index.js",
  "main": "dist/react-file-icon.js",
  "module": "dist/react-file-icon.esm.js",
  "scripts": {
    "build": "microbundle --jsx React.createElement --no-sourcemap",
    "watch": "microbundle --jsx React.createElement --watch",
    "start": "npm run styleguidist",
    "styleguidist": "styleguidist server --config config/styleguide/styleguidist.js",
    "styleguidist:build": "styleguidist build --config config/styleguide/styleguidist.js",
    "test": "jest --config config/jest.js"
  },
  "keywords": [
    "react",
    "component",
    "svg",
    "icons",
    "file types"
  ],
  "license": "MIT",
  "dependencies": {
    "lodash.uniqueid": "^4.0.1",
    "prop-types": "^15.6.0",
    "tinycolor2": "^1.4.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.2",
    "babel-jest": "^22.4.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "component-image": "^2.1.1",
    "jest": "^22.4.3",
    "jest-image-snapshot": "^2.4.1",
    "microbundle": "^0.12.2",
    "react-styleguidist": "^6.5.3",
    "react-test-renderer": "^16.3.2",
    "webpack": "^3.11.0",
    "webpack-blocks": "^1.0.0"
  },
  "peerDependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  }
}
